package team;
/*
 * 인적사항 기본 정보 java beans
 */
public class Human {
	private int number;  //번호
	private String name; //이름
	private int age; //나이
	private Double height; //키

	public Human() {
	}

	public Human(int number, String name, int age, Double height) {
		this.number = number;
		this.name = name;
		this.age = age;
		this.height = height;
	}
	
	
	
	
}
