package main;

import cls.MyClass;

public class MainClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 *  static == 정적
		 * 
		 */

		MyClass cls = new MyClass();
		cls.method(0, null);
		cls.method(0, null);
		cls.method(0, null);
		
		MyClass myCls = new MyClass();
		myCls.method(0, null);
	}

}
